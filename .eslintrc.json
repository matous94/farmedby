{
  "root": true,
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "prettier",
    "prettier/react"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./"]
      }
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "import/prefer-default-export": "off",
    "no-unreachable": "warn",
    "arrow-body-style": "off",
    "react/jsx-boolean-value": "off",
    "react/no-array-index-key": "off",
    "react/prop-types": "warn",
    "prefer-arrow-callback": "off"
  }
}
